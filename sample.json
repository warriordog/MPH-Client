  "miners": [
    {
      "id": "xmr-stak",
      "name": "XMR-stak",
      "path": "miners/xmr-stak",
      "exec": "./xmr-stak",
      "args": "--noAMD --noNVIDIA --currency #COIN# -o #HOST#:#PORT# -u #ACCOUNT#.#WORKER_ID# -p z"
    },
    {
      "id": "ccminer",
      "name": "CCMiner",
      "path": "miners/ccminer",
      "exec": "./ccminer-x64",
      "args": "-r 0 -T #TIMEOUT# -a #ALGORITHM# -o stratum+tcp://#HOST#:#PORT# -u #ACCOUNT#.#WORKER_ID# -p z"
    }
  ],
  "workers": [
    {
      "id": "gpu_nvidia",
      "name": "NVidia GPUs",
      "profit_field": "profit",
      "algorithms": [
        {
          "id": "equihash",
          "miners": [
            {
              "id": "ccminer",
              "rate": "300"
            }
          ]
        }
      ]
    },
    {
      "id": "worker_cpu",
      "name": "CPUs",
      "profit_field": "profit",
