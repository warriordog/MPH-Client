{
  "config_version": 1,
  "settings": {
    "api_key": "",
    "account": "acomputerdog",
    "switch_interval": 60,
    "network_timeout": 60,
    "log_to_file": true,
    "log_level": "DEBUG"
  },
  "workers": [
    {
      "name": "NVidia GPUs",
      "id": "worker_nvidia",
      "profit_field": "profit",
      "algorithms": [
        {
          "name": "Equihash",
          "coins": ["zcash"],
          "rate": 300,
          "miner":{
            "name": "CCMiner",
            "path": "miners/ccminer",
            "exec": "./ccminer-x64",
            "args": "-r 0 -T $$TIMEOUT -a equihash -o stratum+tcp://$$HOST:$$PORT -u $$ACCOUNT.$$WORKER_ID -p z"
          }
        }
      ]
    },
    {
      "name": "CPUs",
      "id": "worker_cpu",
      "profit_field": "profit",
      "algorithms": [
        {
          "name": "Monero",
          "coins": ["monero"],
          "rate": 100,
          "miner": {
            "name": "XMR Stak",
            "path": "miners/xmr-stak",
            "exec": "./xmr-stak",
            "args": "--noAMD --noNVIDIA -o $$HOST:$$PORT -u $$ACCOUNT.$$WORKER_ID -p z"
          }
        }
      ]
    }
  ]
}
