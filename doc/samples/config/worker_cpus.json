{
  "config_version": 7,
  "workers": {
    "cpus": {
      "name": "CPUs",
      "percentProfitThreshold": "1.1",
      "algorithms": {
        "cryptonight": {
          "blacklist_coins": [
            "electroneum"
          ],
          "miners": {
            "xmr-stak": {
              "rate": 100
            }
          }
        }
      },
      "events": [
        {
          "trigger": "worker_startup",
          "action": "log_startup"
        },
        {
          "trigger": "worker_shutdown",
          "action": "log_shutdown"
        },
        {
          "trigger": "coin_switch",
          "action": "log_switch"
        },
        {
          "trigger": "start_mining",
          "action": "log_start_mining"
        },
        {
          "trigger": "stop_mining",
          "action": "log_stop_mining"
        }
      ]
    }
  }
}