{
  "config_version": 6,
  "miners": {
    "xmr-stak": {
      "name": "XMR-stak",
      "path": "miners/xmr-stak",
      "exec": "./xmr-stak",
      "args": "--noAMD --noNVIDIA --currency #COIN# -o #HOST#:#PORT# -u #ACCOUNT#.#WORKER_ID# -p z",
      "remap_coins": {
        "monero": "monero7"
      }
    },
    "ccminer": {
      "name": "CCMiner",
      "path": "miners/ccminer",
      "exec": "./ccminer-x64",
      "args": "-r 0 -T #TIMEOUT# -a #ALGORITHM# -o stratum+tcp://#HOST#:#PORT# -u #ACCOUNT#.#WORKER_ID# -p z"
    },
    "ethminer": {
      "name": "Ethminer",
      "path": "miners/ethminer",
      "exec": "./ethminer",
      "args": ""
    }
  }
}